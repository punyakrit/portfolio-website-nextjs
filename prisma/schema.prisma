
generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}


model User {
  id String @id
  createdAt DateTime @default(now())

  @@index([id])
}

model UserLog{
  id String @id @default(nanoid())
  userId String
  visitCount Int @default(0)
  lastVisitedAt DateTime @default(now())
  createdAt DateTime @default(now())

  @@index([userId])
  @@index([lastVisitedAt])
  @@index([createdAt])
}

